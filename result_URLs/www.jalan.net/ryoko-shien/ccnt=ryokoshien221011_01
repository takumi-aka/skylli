  <script>
    $(function() {
      var menu = $('#jpm__global-nav'),
      offset = menu.offset();
      $(window).scroll(function () {
        if($(window).scrollTop() > offset.top) {
          menu.addClass('jpm__fixed');
        } else {
          menu.removeClass('jpm__fixed');
        }
      });
    });
  </script>
