<script>
var header_obj = $('#floating-header').clone(true).attr('class','header-fixed');
$('#floating-header').after(header_obj);
//
var headerHeight = $("#floating-header").height();
var headerFixHeight = $(".header-fixed").height();
var startPos = 0;
var initFlag = false;
var statusFlag = true;
var movePos = 360;
$(window).scroll(function(){
	var currentPos = $(this).scrollTop();
	if(statusFlag){
		if(initFlag){
			if (currentPos > startPos) {
				if($(window).scrollTop() >= headerHeight) {
					$('#floating-header').css('top','-' + headerHeight + 'px');
					movePos = currentPos;
					statusFlag = false;
				}
			} else if (currentPos < startPos) {
				$('#floating-header').css("top", 0 + "px");
				movePos = currentPos;
				statusFlag = false;
			}
		}
	}else{
		if(currentPos > movePos + 10 || currentPos < movePos - 10){
			statusFlag = true;
		}
	}
	if (currentPos > headerHeight) {
		$('.header-fixed').css('visibility', 'hidden');
		$('#floating-header').css('display', 'block');
		initFlag = true;
	}
	if (currentPos <= $(".header-fixed").scrollTop()) {
		$('.header-fixed').css('visibility', 'visible');
		$('#floating-header').css('display', 'none');
		$('#floating-header').css('top','-' + headerHeight + 'px');
	}
	if (currentPos <= 360) {
		//initFlag = true;
		statusFlag = true;
		$('#floating-header').css('top','-' + headerHeight + 'px');
	}
	startPos = currentPos;
});
</script>
 

